#include <stdio.h>
int main() {
int marks;
printf("Enter the marks:");
scanf("%d",&marks);

 switch(marks/10){
 case 10:
 case 9:
 if(marks>=91)
 printf("Grade A");
 else
 printf("Grade B");
 break;
 case 8:
 if(marks>81)
 printf("Grade B"); 
 else 
 printf("Grade C");
  break;
 case 7:
 if(marks>71)
 printf("Grade C");
 else 
 printf("Grade D");
break;
 case 6:
 if(marks>61)
 printf("Grade D");
 else
 printf("Grade E");
 break;
  case 5:
  if(marks>51)
 printf("Grade E");
 else
 printf("Grade F");
 break;
  case 4:
  if(marks>41)
 printf("Grade F");
 else
 printf("Grade Fail");
 break;
 case 3:
 if(marks>=31&&marks<=39){
    printf("Grade supplementary");
 }
 else{
    printf("Grade fail");
 
 break;
 }
 default:
    printf("Grade fail");

 }
    return 0;
}
