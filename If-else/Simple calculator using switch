#include <stdio.h>
int main() {
int a,b;
char op;
scanf("%c",&op);

scanf("%d %d",&a,&b);

switch(op){
case '+':
printf("Result =%d",a+b);
break;
case'-':
printf("Result =%d",a-b);
break;
case'*':
printf("Result =%d",a*b);
break;
case'/':
if(b==0){
    printf("cant be dvisible");
}
else{
printf("Result =%d",a*b);}
break;
default:
printf("Ivalid operator");}
     return 0;
}
